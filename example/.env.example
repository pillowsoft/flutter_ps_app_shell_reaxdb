# Flutter App Shell - Environment Configuration Template
# Copy this file to .env and configure your values

# InstantDB Configuration
# Leave INSTANTDB_APP_ID empty for local-only mode
# Get your app ID from https://www.instantdb.dev/dash
INSTANTDB_APP_ID=

# Sync Configuration (only applies when app ID is provided)
INSTANTDB_ENABLE_SYNC=true
INSTANTDB_VERBOSE_LOGGING=false

# Optional: Force local-only mode even with app ID
# Uncomment the line below to disable cloud sync
# FORCE_LOCAL_ONLY=true